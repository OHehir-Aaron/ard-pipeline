			<METADATA KEY="XML,EODS_DATASET,EXEXTENT,LR_LAT"/>
			<METADATA KEY="XML,METADATA,EXTENT,LR_LAT"/>
			<METADATA KEY="REPORT,LR_LAT" FORMAT="%11.7f"/>
		</VARIABLE>

		<VARIABLE NAME="lr_lon" TYPE="float">
