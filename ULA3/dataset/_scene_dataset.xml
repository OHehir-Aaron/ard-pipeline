			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,CORNER_LL_LON_PRODUCT"/>
			<METADATA KEY="XML,EODS_DATASET,EXEXTENT,LL_LONG"/>
			<METADATA KEY="XML,METADATA,EXTENT,LL_LONG"/>
			<METADATA KEY="REPORT,LL_LONG" FORMAT="%11.7f"/>
