			<METADATA KEY="XML,EODS_DATASET,GRIDSPATIALREPRESENTATION,GEORECTIFIED,GEOREFURPOINT_X"/>
			<METADATA KEY="REPORT,UR_EAST" FORMAT="%11.3f"/>
