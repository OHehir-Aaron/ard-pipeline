		<VARIABLE NAME="ll_x" TYPE="float" REQUIRED="False">
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,PRODUCT_LL_CORNER_MAPX"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,CORNER_LL_PROJECTION_X_PRODUCT"/>
			<METADATA KEY="XML,EODS_DATASET,GRIDSPATIALREPRESENTATION,GEORECTIFIED,GEOREFLLPOINT_X"/>
