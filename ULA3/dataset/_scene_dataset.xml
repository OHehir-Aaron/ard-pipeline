			<METADATA KEY="XML,METADATA,EXTENT,LR_LONG"/>
			<METADATA KEY="REPORT,LR_LONG" FORMAT="%11.7f"/>
		</VARIABLE>

		<VARIABLE NAME="ul_x" TYPE="float" REQUIRED="False">
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,PRODUCT_UL_CORNER_MAPX"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,CORNER_UL_PROJECTION_X_PRODUCT"/>
			<METADATA KEY="XML,EODS_DATASET,GRIDSPATIALREPRESENTATION,GEORECTIFIED,GEOREFULPOINT_X"/>
			<METADATA KEY="REPORT,UL_EAST" FORMAT="%11.3f"/>
		</VARIABLE>

		<VARIABLE NAME="ul_y" TYPE="float" REQUIRED="False">
