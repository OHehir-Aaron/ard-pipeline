<?xml version="1.0" encoding="UTF-8" ?>
<GA_DATASET>
	<CLASS NAME="SceneDataset">
		<DATA>
			<DATADIR PATH="scene01"/>
			<DATADIR PATH="."/>

			<FILETYPE EXTENSION="TIF" ROOTSUFFIX="B%d" PRODUCTFORMAT="GeoTIFF"/>
			<FILETYPE EXTENSION="FST" ROOTSUFFIX="HRF" PRODUCTFORMAT="FASTL7A"/>
		</DATA>

        <METADATA_SOURCE TYPE="XML" REQUIRED="False">
			<FILE PATTERN="/metadata\.xml$"/>
			<FILE PATTERN="/EODS_metadata\.xml$"/>
			<FILE PATTERN="/scene01/L.*_L1T\.xml$"/>
			<FILE PATTERN="/L.*_L1T\.xml$"/>
			<FILE PATTERN="/scene01/L.*_L1G\.xml$"/>
			<FILE PATTERN="/L.*_L1G\.xml$"/>
		</METADATA_SOURCE>

		<METADATA_SOURCE TYPE="MTL" REQUIRED="False">
			<FILE PATTERN="scene01/L.*_MTL\.txt$"/>
			<FILE PATTERN="L.*_MTL\.txt$"/>
		</METADATA_SOURCE>

		<METADATA_SOURCE TYPE="REPORT" REQUIRED="False">
			<FILE PATTERN="scene01/report\.txt$"/>
			<FILE PATTERN="report\.txt$"/>
		</METADATA_SOURCE>


		<VARIABLE NAME="satellite_name" TYPE="string" REQUIRED="True">
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,SPACECRAFT_ID"/>
			<METADATA KEY="XML,EODS_DATASET,ACQUISITIONINFORMATION,PLATFORMNAME"/>
			<METADATA KEY="FST,SATELLITE"/>
			<METADATA KEY="REPORT,SATELLITE"/>
		</VARIABLE>

		<VARIABLE NAME="sensor">
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,SENSOR_ID"/>
			<METADATA KEY="XML,EODS_DATASET,ACQUISITIONINFORMATION,INSTRUMENTNAME"/>
			<METADATA KEY="XML,METADATA,IMAGE_DESCRIPTION,INSTRUMENT"/>
			<METADATA KEY="REPORT,SENSOR"/>
		</VARIABLE>

		<VARIABLE NAME="sensor_mode" REQUIRED="False">
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,SENSOR_MODE"/>
			<METADATA KEY="XML,EODS_DATASET,IMAGEDESCRIPTION,SENSOROPERATIONMODE"/>
			<METADATA KEY="XML,METADATA,IMAGE_DESCRIPTION,OPERATION_MODE"/>
			<METADATA KEY="REPORT,SENSOR_MODE"/>
		</VARIABLE>

		<VARIABLE NAME="ground_station" TYPE="string" REQUIRED="False">
			<METADATA KEY="XML,EODS_DATASET,ACQUISITIONINFORMATION,EVENT,COLLECTIONSITE"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,METADATA_FILE_INFO,GROUND_STATION"/>
			<METADATA KEY="XML,METADATA,IMAGE_DESCRIPTION,RECEIVING_ANTENNA"/>
			<METADATA KEY="REPORT,GROUND_STATION"/>
		</VARIABLE>

		<VARIABLE NAME="map_projection">
			<METADATA KEY="MTL,L1_METADATA_FILE,PROJECTION_PARAMETERS,MAP_PROJECTION"/>
			<METADATA KEY="XML,EODS_DATASET,GRIDSPATIALREPRESENTATION,GEORECTIFIED,PROJECTION"/>
			<METADATA KEY="XML,METADATA,SPATIAL_REP,MAP_PROJECTION"/>
			<METADATA KEY="REPORT,MAP_PROJECTION"/>
		</VARIABLE>

		<VARIABLE NAME="zone" TYPE="int" REQUIRED="False">
			<METADATA KEY="MTL,L1_METADATA_FILE,UTM_PARAMETERS,ZONE_NUMBER"/>
			<METADATA KEY="XML,EODS_DATASET,GRIDSPATIALREPRESENTATION,GEORECTIFIED,ZONE"/>
			<METADATA KEY="XML,METADATA,SPATIAL_REP,ZONE"/>
			<METADATA KEY="REPORT,ZONE"/>
		</VARIABLE>

		<VARIABLE NAME="scan_gap_corr" TYPE="int" REQUIRED="False">
			<METADATA KEY="MTL,L1_METADATA_FILE,PROJECTION_PARAMETERS,SCAN_GAP_INTERPOLATION"/>
			<METADATA KEY="XML,METADATA,PROCESSING_PARAMETERS,SCAN_GAP_CORRECTION"/>
		</VARIABLE>

		<VARIABLE NAME="image_lines" TYPE="int">
			<METADATA KEY="XML,EODS_DATASET,GRIDSPATIALREPRESENTATION,DIMENSION_Y,SIZE"
				REGEX="^\d*" GROUP="0"/>
			<METADATA KEY="XML,METADATA,SPATIAL_REP,Y_PIXELS_SIZE"/>
			<METADATA KEY="REPORT,IMAGE_LINES"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,PRODUCT_LINES_REF"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,REFLECTIVE_LINES"/>
		</VARIABLE>

		<VARIABLE NAME="image_pixels" TYPE="int">
			<METADATA KEY="XML,EODS_DATASET,GRIDSPATIALREPRESENTATION,DIMENSION_X,SIZE"
				REGEX="^\d*" GROUP="0"/>
			<METADATA KEY="XML,METADATA,SPATIAL_REP,X_PIXELS_SIZE"/>
			<METADATA KEY="REPORT,IMAGE_PIXELS"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,PRODUCT_SAMPLES_REF"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,REFLECTIVE_SAMPLES"/>
		</VARIABLE>

		<VARIABLE NAME="image_orientation">
			<METADATA KEY="MTL,L1_METADATA_FILE,PROJECTION_PARAMETERS,ORIENTATION"/>
			<METADATA KEY="XML,EODS_DATASET,DATAQUALITY,LINEAGE,STATEMENT"
				REGEX="(?&lt;=Orientation=)([^,]+)" GROUP="1"/>
			<METADATA KEY="XML,METADATA,PROCESSING,ORIENTATION"/>
			<METADATA KEY="REPORT,IMAGE_ORIENTATION"/>
		</VARIABLE>

		<VARIABLE NAME="scene_centre_date" TYPE="date">
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,ACQUISITION_DATE" FORMAT="%Y-%m-%d"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,DATE_ACQUIRED" FORMAT="%Y-%m-%d"/>
			<METADATA KEY="REPORT,SCENE_CENTRE_DATE" FORMAT="%Y-%m-%d"/>
			<METADATA KEY="REPORT,PRODUCT_SCENE_CENTRE_DATETIME_YYYYMMDD_HHMMSSSS"
				REGEX="\d{4}-\d{2}-\d{2}" FORMAT="%Y-%m-%d"/>
		</VARIABLE>

		<VARIABLE NAME="scene_centre_time" TYPE="time">
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,SCENE_CENTER_SCAN_TIME"
				REGEX="\d{2}:\d{2}:\d{2}\.\d{1,6}" GROUP="0" FORMAT="%H:%M:%S.%f"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,SCENE_CENTER_TIME"
				REGEX="\d{2}:\d{2}:\d{2}\.\d{1,6}" GROUP="0" FORMAT="%H:%M:%S.%f"/>
			<METADATA KEY="REPORT,SCENE_CENTRE_TIME"
				REGEX="\d{2}:\d{2}:\d{2}\.\d{1,6}" GROUP="0" FORMAT="%H:%M:%S.%f"/>
			<METADATA KEY="REPORT,PRODUCT_SCENE_CENTRE_DATETIME_YYYYMMDD_HHMMSSSS"
				REGEX="\d{2}:\d{2}:\d{2}\.\d{1,6}" FORMAT="%H:%M:%S.%f"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,SCENE_CENTER_SCAN_TIME"
				REGEX="\d{2}:\d{2}:\d{2}" GROUP="0" FORMAT="%H:%M:%S"/>
			<METADATA KEY="REPORT,SCENE_CENTRE_TIME"
				REGEX="\d{2}:\d{2}:\d{2}" GROUP="0" FORMAT="%H:%M:%S"/>
			<METADATA KEY="REPORT,PRODUCT_SCENE_CENTRE_DATETIME_YYYYMMDD_HHMMSSSS"
				REGEX="\d{2}:\d{2}:\d{2}" FORMAT="%H:%M:%S"/>
		</VARIABLE>

		<VARIABLE NAME="sun_elevation" TYPE="float" REQUIRED="False">
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_PARAMETERS,SUN_ELEVATION"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,IMAGE_ATTRIBUTES,SUN_ELEVATION"/>
			<METADATA KEY="XML,METADATA,IMAGE_DESCRIPTION,SUN_ELEVATION"/>
			<METADATA KEY="REPORT,SUN_ELEVATION" REGEX="(\d*\.?\d*)(?=\s+deg)" FORMAT="%.3f"/>
		</VARIABLE>

		<VARIABLE NAME="sun_azimuth" TYPE="float" REQUIRED="False">
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_PARAMETERS,SUN_AZIMUTH"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,IMAGE_ATTRIBUTES,SUN_AZIMUTH"/>
			<METADATA KEY="XML,METADATA,IMAGE_DESCRIPTION,SUN_AZIMUTH"/>
			<METADATA KEY="REPORT,SUN_AZIMUTH" REGEX="(\d*\.?\d*)(?=\s+deg)" FORMAT="%.3f"/>
		</VARIABLE>

		<VARIABLE NAME="product_format">
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_PARAMETERS,OUTPUT_FORMAT"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,OUTPUT_FORMAT"/>
			<METADATA KEY="XML,EODS_DATASET,MDRESOURCE,FORMAT,FORMATNAME"/>
			<METADATA KEY="XML,METADATA,DATASET,PRODUCT_FORMAT"/>
			<METADATA KEY="REPORT,PRODUCT_FORMAT"/>
		</VARIABLE>

		<VARIABLE NAME="resampling" REQUIRED="False">
			<METADATA KEY="MTL,L1_METADATA_FILE,PROJECTION_PARAMETERS,RESAMPLING_OPTION"/>
			<METADATA KEY="XML,EODS_DATASET,DATAQUALITY,LINEAGE,STATEMENT"
				REGEX="(?&lt;=Resampling=)([^,]+)" GROUP="1"/>
		</VARIABLE>

		<VARIABLE NAME="completion_date" TYPE="date" REQUIRED="False">
			<METADATA KEY="XML,EODS_DATASET,MDRESOURCE,CITATION,DATE"
				REGEX="\d{4}-\d{2}-\d{2}" FORMAT="%Y-%m-%d"/>
			<METADATA KEY="XML,EODS_DATASET,MDRESOURCE,CITATION,DATE"
				REGEX="\d{4}\d{2}\d{2}" FORMAT="%Y%m%d"/>
			<METADATA KEY="XML,METADATA,CITATION,CITATION_DATE"
				REGEX="\d{4}-\d{2}-\d{2}" FORMAT="%Y-%m-%d"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,METADATA_FILE_INFO,PRODUCT_CREATION_TIME"
				REGEX="\d{4}-\d{2}-\d{2}" FORMAT="%Y-%m-%d"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,METADATA_FILE_INFO,FILE_DATE"
				REGEX="\d{4}-\d{2}-\d{2}" FORMAT="%Y-%m-%d"/>
			<METADATA KEY="REPORT,COMPLETION_DATE" FORMAT="%Y-%m-%d"/>
		</VARIABLE>

		<VARIABLE NAME="completion_time" TYPE="time" REQUIRED="False">
			<METADATA KEY="XML,METADATA,CITATION,CITATION_DATE"
				REGEX="\d{2}:\d{2}:\d{2}" FORMAT="%H:%M:%S"/>
			<METADATA KEY="XML,EODS_DATASET,MDRESOURCE,CITATION,DATE"
				REGEX="\d{2}:\d{2}:\d{2}" FORMAT="%H:%M:%S"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,METADATA_FILE_INFO,PRODUCT_CREATION_TIME"
				REGEX="\d{2}:\d{2}:\d{2}" FORMAT="%H:%M:%S"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,METADATA_FILE_INFO,FILE_DATE"
				REGEX="TO\d{2}:\d{2}:\d{2}Z" FORMAT="%H:%M:%S"/>
			<METADATA KEY="REPORT,COMPLETION_TIME" FORMAT="%H:%M:%S"/>
		</VARIABLE>

		<VARIABLE NAME="ul_lat" TYPE="float">
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,PRODUCT_UL_CORNER_LAT"/>
			<METADATA KEY="MTL,L1_METADATA_FILE,PRODUCT_METADATA,CORNER_UL_LAT_PRODUCT"/>
			<METADATA KEY="XML,EODS_DATASET,EXEXTENT,UL_LAT"/>
			<METADATA KEY="XML,METADATA,EXTENT,UL_LAT"/>
			<METADATA KEY="REPORT,UL_LAT" FORMAT="%11.7f"/>
		</VARIABLE>

		<VARIABLE NAME="ul_lon" TYPE="float">
