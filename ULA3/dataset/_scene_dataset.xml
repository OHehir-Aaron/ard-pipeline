			<METADATA KEY="REPORT,PRODUCT_SCENE_CENTRE_LOCATION_LATLONG" REGEX="(-?\d+\.?\d*)\s*/\s*(-?\d+\.?\d*)" GROUP="1" FORMAT="%.3f"/>
		</VARIABLE>

		<VARIABLE NAME="scene_centre_long" TYPE="float">
			<METADATA KEY="XML,EODS_DATASET,EXEXTENT,SCENECENTRELONG"/>
