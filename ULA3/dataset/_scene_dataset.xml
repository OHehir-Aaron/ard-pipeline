			<METADATA KEY="XML,EODS_DATASET,EXEXTENT,UR_LONG"/>
			<METADATA KEY="XML,METADATA,EXTENT,UR_LONG"/>
			<METADATA KEY="REPORT,UR_LONG" FORMAT="%11.7f"/>
		</VARIABLE>

		<VARIABLE NAME="ll_lat" TYPE="float">
